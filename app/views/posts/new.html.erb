<% @post.errors.each do |error| %>
  <%= error.full_message %>
<% end %>

<%= form_with model: @post do |form| %>
  <p>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </p>
  <p>
    <%= form.label :body %>
    <%= form.text_area :body%>
  </p>
  <p>
    <%= form.hidden_field :user_id, value: current_user.id %>
  </p>
  <p>
    <%= form.submit%>
  </p>
<% end %>